        <activity
            android:name=".activitys.BookingForOpenDoor"
            android:label="@string/title_activity_booking_for_open_door" >
        </activity>
        <activity
            android:name=".activitys.ReportErrorActivity"
            android:exported="true"
            android:label="@string/title_activity_report_error"
            android:process=":reporterror" >
        </activity>
        <activity
            android:name=".activitys.CheckBookingActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".activitys.OrderFormActivity"
            android:screenOrientation="landscape" >
